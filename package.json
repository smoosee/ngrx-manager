{
  "name": "@smoosee/ngrx-manager",
  "version": "0.18.0",
  "description": "Plug-N-Play manager for NGRX Stores.",
  "repository": {
    "type": "git",
    "url": "smoosee/ngrx-manager"
  },
  "keywords": [
    "smoosee",
    "angular",
    "ngrx",
    "signals",
    "facade",
    "manager"
  ],
  "author": "Mostafa Sherif",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smoosee/ngrx-manager/issues"
  },
  "homepage": "https://github.com/smoosee/ngrx-manager#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "************* DEV ****************": "",
    "start": "ng serve demo",
    "watch": "yarn build --watch",
    "test": "ng test",
    "************* BUILD ****************": "",
    "build": "ng build @smoosee/ngrx-manager",
    "************* RELEASE ****************": "",
    "docs": "typedoc --plugin typedoc-plugin-markdown --out docs lib/src/index.ts",
