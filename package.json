{
  "name": "@smoosee/ngrx-manager",
  "version": "0.17.2",
  "description": "Plug-N-Play manager for NGRX Stores.",
  "repository": {
    "type": "git",
    "url": "smoosee/ngrx-manager"
  },
  "engines": {
    "npm": ">=8.0.0 <10.0.0",
    "node": ">=16.0.0 <20.0.0"
  },
  "keywords": [
    "smoosee",
    "angular",
    "ngrx",
    "signals",
    "facade",
    "manager"
  ],
  "author": "Mostafa Sherif",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smoosee/ngrx-manager/issues"
  },
  "homepage": "https://github.com/smoosee/ngrx-manager#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "************* DEV ****************": "",
    "start": "ng serve demo",
    "watch": "yarn build --watch",
    "test": "ng test",
    "************* BUILD ****************": "",
    "build": "ng build @smoosee/ngrx-manager",
    "************* RELEASE ****************": "",
    "docs": "typedoc --plugin typedoc-plugin-markdown --out docs lib/src/index.ts",
    "release": "auto shipit",
    "prepack": "indeps prepack && npm version patch --no-git-tag-version",
    "postpack": "git reset --hard HEAD"
  },
  "peerDependencies": {
    "@angular/core": "17.3.12",
    "@ngrx/effects": "17.2.0",
    "@ngrx/store": "17.2.0"
  },
  "dependencies": {
    "@angular/animations": "17",
    "@angular/common": "17",
    "@angular/compiler": "17",
    "@angular/core": "17.3.12",
    "@angular/forms": "17",
    "@angular/platform-browser": "17",
    "@angular/platform-browser-dynamic": "17",
    "@angular/router": "17",
    "@ngrx/effects": "17.2.0",
    "@ngrx/store": "17.2.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "^0.14.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17",
    "@angular/cli": "17",
    "@angular/compiler-cli": "17",
    "@smoosee/indeps": "latest",
