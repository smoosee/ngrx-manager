    "postpack": "git reset --hard HEAD"
  },
  "peerDependencies": {
    "@angular/core": "^18.2.5",
    "@ngrx/effects": "^18.0.0",
    "@ngrx/store": "^18.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.5",
    "@angular/animations": "^18.2.5",
    "@angular/cli": "^18.2.5",
    "@angular/common": "^18.2.5",
    "@angular/compiler": "^18.2.5",
    "@angular/compiler-cli": "^18.2.5",
    "@angular/core": "^18.2.5",
    "@angular/forms": "^18.2.5",
    "@angular/platform-browser": "^18.2.5",
    "@angular/platform-browser-dynamic": "^18.2.5",
    "@angular/router": "^18.2.5",
    "@ngrx/effects": "^18.0.0",
    "@ngrx/store": "^18.0.0",
    "@smoosee/indeps": "latest",
    "@types/jest": "^29.5.12",
    "auto": "^11.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-preset-angular": "^14.1.1",
    "jsdoc-to-markdown": "^8.0.1",
    "ng-packagr": "^18.2.1",
    "rxjs": "~7.8.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typedoc": "^0.26.4",
    "typedoc-plugin-markdown": "^4.2.1",
    "typescript": "~5.5.4",
    "zone.js": "^0.14.8"
