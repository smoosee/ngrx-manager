    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-preset-angular": "^14.1.1",
    "jsdoc-to-markdown": "^8.0.1",
    "ng-packagr": "17",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.4",
    "typedoc-plugin-markdown": "^4.2.1",
    "typescript": "5.4"
  },
  "packageManager": "yarn@4.2.2"
